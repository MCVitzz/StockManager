<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Username" />
      <TextField fx:id="usernameTF">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Label text="Password" />
      <PasswordField fx:id="passwordPF">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </PasswordField>
      <Button mnemonicParsing="false" onAction="#login" text="Login" />
   </children>
</VBox>
